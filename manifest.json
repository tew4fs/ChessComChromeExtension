{
    "name": "Chess.com Ratings Extension",
    "description": "Views your Chess.com ratings straight from the extension",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "scripting", "activeTab"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "chessLogo.png",
            "32": "chessLogo.png",
            "48": "chessLogo.png",
            "128": "chessLogo.png"
          }
        },
        "icons": {
          "16": "chessLogo.png",
          "32": "chessLogo.png",
          "48": "chessLogo.png",
          "128": "chessLogo.png"
        }
  }